<mvc:View id="_IDGenView1" height="100%" controllerName="aca20241q.controller.LuthierDetail"
    xmlns="sap.m"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns:form="sap.ui.layout.form">
    <Dialog id="_IDGenDialog1" title="Luthier {Nombre} {Apellido}" state="Information" titleAlignment="Center" contentWidth="30%">
        <content>
            <Table id="tablitaDetalle" items="{ToInstrumentoSet}">
                <columns>
                    <Column id="col1">
                        <Text id="text1" text="Id Luthier" />
                    </Column>
                    <Column id="col2">
                        <Text id="text2" text="Nombre" />
                    </Column>
                    <Column id="col3">
                        <Text id="text3" text="Apellido" />
                    </Column>
                </columns>
                <ColumnListItem id="collistitem">
                    <cells>
                        <Text id="_IDGenText4" text="{IdLuthier}"/>
                        <Text id="_IDGenText5" text="{NombreInstrumento}"/>
                        <Text id="_IDGenText6" text="{TipoInstrumento}"/>
                    </cells>
                </ColumnListItem>
            </Table>
        </content>
        <beginButton>
            <Button id="_IDGenButton1" text="Agregar" press=".onCreateLuthierPress" type="Emphasized" />
        </beginButton>
        <endButton>
            <Button id="_IDGenButton2" text="Cancelar" press=".onCloseLuthierPress" type="Reject" />
        </endButton>
    </Dialog>
</mvc:View>