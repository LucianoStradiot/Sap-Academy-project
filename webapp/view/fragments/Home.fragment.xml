<core:FragmentDefinition xmlns="sap.suite.ui.commons"
    xmlns:core="sap.ui.core"
    xmlns:viz="sap.viz.ui5.controls"
    xmlns:layout="sap.ui.layout">
    <layout:Splitter id="Home" width="100%" height="100%">
        <layout:contentAreas>
            <ChartContainer id="chartContainer" showFullScreen="true" showPersonalization="false" autoAdjustHeight="false" personalizationPress="attachPersonalizationPress" contentChange="attachContentChange" title="Revenue">
                <content>
                    <ChartContainerContent id="_IDGenChartContainerContent1" icon="sap-icon://line-chart" title="Line Chart">
                        <content>
                            <viz:VizFrame id="_IDGenVizFrame1" height="700px" width="100%" uiConfig="{applicationSet:'fiori'}"></viz:VizFrame>
                        </content>
                    </ChartContainerContent>
                </content>
            </ChartContainer>
        </layout:contentAreas>
    </layout:Splitter>
</core:FragmentDefinition>