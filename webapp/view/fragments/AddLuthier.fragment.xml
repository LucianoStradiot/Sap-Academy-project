                    <Input id="nombreInput" value="{CreateLuthier>/Nombre}" valueLiveUpdate="true"/>
                    <Label id="_IDGenLabel3" text="Apellido" />
                    <Input id="apellidoInput" value="{CreateLuthier>/Apellido}" valueLiveUpdate="true"/>
                    <Label id="_IDGenLabel4" text="Localidad" />
                    <Input id="localidadInput" value="{CreateLuthier>/Localidad}" valueLiveUpdate="true"/>
                    <Label id="_IDGenLabel5" text="Tipo de Instrumento" />
                    <Input id="tipoInstrumentoInput" value="{CreateLuthier>/TipoInstrumento}" valueLiveUpdate="true"/>
                    <Label id="_IDGenLabel6" text="DescripciÃ³n" />
                    <TextArea id="descripcionInput" value="{CreateLuthier>/Descripcion}" valueLiveUpdate="true"/>
                </form:content>
                <form:layoutData>
                    <FlexItemData id="_IDGenFlexItemData1" shrinkFactor="0" backgroundDesign="Solid" styleClass="sapContrastPlus"/>
